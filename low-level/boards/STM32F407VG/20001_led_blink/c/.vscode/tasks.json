{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Open dspcoder panel",
        "type": "shell",
        "command": "${command:dspcoder.showPanel}",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "presentation": {
          "reveal": "always",
          "panel": "new"
        },
        "runOptions": {
          "runOn": "folderOpen"
        },
        "dependsOrder": "sequence",
        "dependsOn": [
          "Open main.c file"
        ]
      },
      {
        "label": "Open main.c file",
        "type": "shell",
        "command": "code-server",
        "args": ["${workspaceFolder}/Src/main.c"],
        "presentation": {
        "reveal": "never",
        "close": true
        },
        "problemMatcher": []
      }
    ]
  }